
public abstract class Account {
	
	private double amount;

	public Account() {
		super();
	}

	public Account(double amount) {
		super();
		this.amount = amount;
	}

	public double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}
	
	public abstract void invalidAccount(Account obj) throws InvalidAccountException;
	

}
