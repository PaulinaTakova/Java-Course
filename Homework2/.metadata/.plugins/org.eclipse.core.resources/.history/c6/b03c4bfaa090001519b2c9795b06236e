import java.util.Scanner;

public class EmployeesInCompany {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner("System.in");
		
		String firstName;
		String lastName;
		short years;
		char gender;
		long egn;
		int phoneNumber;
		
		System.out.println("Enter the first name of the employee:");
		firstName = sc.nextLine();
		System.out.println("Enter the last name of the employee:");
		lastName = sc.nextLine();
		System.out.println("Enter the years of the employee:");
		years = sc.nextShort();
		System.out.println("Enter the gender of the employee:");
		gender = sc.next().charAt(0);
		System.out.println("Enter the EGN of the employee:");
		egn = sc.nextLong();
		System.out.println("Enter the phone number of the employee:");
		phoneNumber = sc.nextInt();
		
		System.out.println("");
		System.out.println(firstName + " " + lastName + " " + years + " " + gender + " " + egn + " " + phoneNumber);
		
		sc.close();
	}

}
