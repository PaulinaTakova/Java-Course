
public class From30To1 {

	public static void main(String[] args) {
		
		int[] arr = new int[30];
		
		for (int i = 0; i < arr.length; i++) {
			arr[i] = arr.length-1-i;
		}
		
		for (int i = 0; i < arr.length ; i++) {
			System.out.print(arr[i] + " ");
			if(arr[i] == 6 || arr[i] == 12 || arr[i] == 18 || arr[i] == 24 ){
				System.out.println();
			}
		}
		
	}

}
