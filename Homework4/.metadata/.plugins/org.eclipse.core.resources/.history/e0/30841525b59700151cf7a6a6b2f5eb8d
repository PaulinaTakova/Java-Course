
public class From30To1 {

	public static void main(String[] args) {
		
		int[] arr = new int[30];
		
		for (int i = 0; i < arr.length; i++) {
			arr[i] = arr.length-1;
		}
		
		for (int i = 0; i < arr.length ; i++) {
			System.out.print(arr[i] + " ");
			if(arr[i] == 7 || arr[i] == 13 || arr[i] == 19 || arr[i] == 25 ){
				System.out.println();
			}
		}
		
	}

}
