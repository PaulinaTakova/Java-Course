import java.util.Scanner;

public class SymmetricArray {

	public static void main(String[] args) {
		int[] arr = new int[10];
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter 10 numbers for array:");
		
		for (int i = 0; i < arr.length; i++) {
			arr[i] = sc.nextInt();
		}
	}

}
