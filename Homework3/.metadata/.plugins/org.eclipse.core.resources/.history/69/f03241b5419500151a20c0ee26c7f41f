import java.util.Scanner;

public class TheBiggestNum {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int max = 0;
		
		for(int i = 0; i<=5; i++){
			int a = sc.nextInt();
			if(a > i){
				 max = a;
			}
		}
		
		System.out.println("The biggest number is: " + max);
	}

}
