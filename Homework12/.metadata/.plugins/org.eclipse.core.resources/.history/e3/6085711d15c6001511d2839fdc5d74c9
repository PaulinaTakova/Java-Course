package DesignPattern;

public class Phone extends Device {
	
	private int memory;

	public Phone() {
		super();
	}
	
	public Phone(String model, String brand, int memory) {
		super(model, brand);
		this.memory = memory;
	}

	public int getMemory() {
		return memory;
	}

	public void setMemory(int memory) {
		this.memory = memory;
	}
	
	public void infoAboutDevice(Phone p){
		System.out.println("The model:" + getModel() + "The brand: " + getBrand() + "Memory: " + getMemory());
	}

}
