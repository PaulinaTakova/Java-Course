
public class Car {
	
	private int price;
	
	public Car(int price){
		this.price = price;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	public void sortByPrice(int price){
		for (int i = 0; i < car.length; i++) {
			for (int j = 1; j < car.length; j++) {
				if(car[i].price > car[j].price){
					int a = car[i];
					car[i] = car[j];
					car[j] = car[i];
				}
			}
		}
	}
	

}
